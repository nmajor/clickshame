{
    "name": "__MSG_appName__",
    "version": "0.0.10",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        },
        "default_title": "Click Shame Beta"
    },
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "css": [
                "styles/strike.css"
            ],
            "js": [
                "scripts/contentscript.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "permissions": [
        "tabs",
        "background",
        "activeTab"
    ],
    "web_accessible_resources": ["strike.html"]
}